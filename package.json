{
  "name": "@aligent/microservice-development-utilities",
  "version": "1.0.0",
  "description": "A set of utilities for Aligent Microservices development",
  "type": "module",
  "scripts": {
    "check-types": "nx affected -t check-types",
    "check-types:all": "nx run-many -t check-types",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "lint": "nx affected -t lint",
    "lint:all": "nx run-many -t lint",
    "test": "nx affected -t test --coverage",
    "test:all": "nx run-many -t test --coverage",
    "build": "nx run affected -t build",
    "build:all": "nx run run-many -t build",
    "docs": "nx run affected -t docs",
    "docs:all": "nx run run-many -t docs"
  },
  "author": "Aligent",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@aws-sdk/client-ssm": "^3.682.0",
    "axios": "^1.7.7",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@aligent/ts-code-standards": "^1.3.1",
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "20.0.7",
    "@nx/eslint-plugin": "^20.0.7",
    "@nx/js": "^20.0.9",
    "@nx/vite": "20.0.7",
    "@nx/web": "20.0.7",
    "@nx/workspace": "^20.0.7",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/lodash": "^4.17.13",
    "@types/node": "18.16.9",
    "@types/object-hash": "^3.0.6",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "aws-sdk-client-mock": "^2.1.1",
    "aws-sdk-client-mock-jest": "^2.1.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.0.0",
    "nx": "20.0.7",
    "prettier": "^3.3.3",
    "tslib": "^2.3.0",
    "typedoc": "^0.26.10",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.0.0",
    "verdaccio": "^5.0.4",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.1.4"
  },
  "files": [
    "dist",
    "docs"
  ],
  "nx": {
    "includedScripts": []
  }
}
