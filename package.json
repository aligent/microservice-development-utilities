{
  "name": "@aligent/microservice-util-lib",
  "version": "1.0.0",
  "description": "A set of utility functions for Aligent Microservices",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run docs",
    "lint": "eslint src/*.ts src/**/*.ts",
    "lint:fix": "eslint --fix src/*.ts src/**/*.ts",
    "test": "npm run lint && npm run test:types && npm run test:jest",
    "test:jest": "nx jest:test src/",
    "test:types": "tsc --noEmit",
    "docs": "typedoc --readme none --githubPages false --plugin typedoc-plugin-markdown --entryDocument modules.md --namedAnchors true --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/aligent/microservice-util-lib.git"
  },
  "author": "Aligent",
  "license": "ISC",
  "devDependencies": {
    "@aligent/serverless-conventions": "latest",
    "@nx/devkit": "^17.1.3",
    "@nx/esbuild": "17.1.3",
    "@nx/eslint": "19.7.3",
    "@nx/eslint-plugin": "19.7.3",
    "@nx/jest": "19.7.3",
    "@nx/js": "19.7.3",
    "@nx/linter": "19.7.3",
    "@nx/plugin": "^19.1.2",
    "@nx/vite": "19.7.3",
    "@nx/web": "19.7.3",
    "@nx/workspace": "19.7.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.2",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "20.12.12",
    "@types/object-hash": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "aws-sdk-client-mock": "^2.1.1",
    "aws-sdk-client-mock-jest": "^2.1.1",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "nx": "19.7.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "~5.6.2",
    "vite": "^5.0.0",
    "vitest": "^1.3.1"
